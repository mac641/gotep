{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Debug run command from main.go",
			"type": "go",
			"request": "launch",
			"mode": "debug",
			"program": "${workspaceFolder}/src/main.go",
			"env": {},
			"args": [
				"run",
				"-c",
				"${workspaceFolder}/tests/test_files/http-client.env.json",
				"-f",
				"${workspaceFolder}/tests/test_files/default.http"
			]
		},
		{
			"name": "Debug httpSpec grammar",
			"type": "antlr-debug",
			"request": "launch",
			"input": "tests/test_files/default.http",
			"grammar": "src/lib/parser/httpSpec.g4",
			"startRule": "file",
			"printParseTree": true,
			"visualParseTree": true
		}
	]
}
